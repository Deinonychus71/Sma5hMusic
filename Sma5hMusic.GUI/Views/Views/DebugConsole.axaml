<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Sma5hMusic.GUI.Views.DebugConsole">
  <Grid>
    <Grid Classes="debugConsole" Opacity="0.80" RowDefinitions="*" ColumnDefinitions="*"/>
    <ScrollViewer x:Name="scrollConsole" VerticalScrollBarVisibility="Auto">
      <TextBox x:Name="txtConsole" IsReadOnly="True" AcceptsReturn="True" />
    </ScrollViewer>
  </Grid>
</UserControl>
